    "karma-phantomjs-launcher": "^1.0.4",
    "rxjs": "^5.3.1",
    "systemjs": "^0.20.12",
    "tslint": "^3.15.1",
    "tslint-ionic-rules": "^0.0.6",
    "typescript": "^2.3.2",
    "zone.js": "^0.8.10"
